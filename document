# sudo -S phpDocumentor --ignore "*/mocks/*" --visibility="public" -d $HOME/Projects/RAMP/code/,$HOME/Projects/RAMP/tests/,$HOME/Projects/RAMP/make -t $HOME/Code/Documentation/php
